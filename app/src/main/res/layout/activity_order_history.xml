<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@color/white"
tools:context=".views.Profile.orderhistory.OrderHistoryActivity">
<RelativeLayout
    android:id="@+id/actionbarOrderHistory"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:layout_marginTop="5dp"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    app:layout_constraintStart_toStartOf="parent">
    <ImageView
        android:id="@+id/orderHistoryBack"
        android:src="@drawable/ic_back"
        android:layout_width="22dp"
        android:layout_marginEnd="3dp"
        android:layout_marginStart="10dp"
        android:layout_centerVertical="true"
        android:layout_height="22dp">
    </ImageView>
    <TextView
        android:layout_width="wrap_content"
        android:text="Lịch Sử Đổi Thưởng"
        android:textSize="24dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:layout_height="wrap_content">
    </TextView>
</RelativeLayout>
<androidx.recyclerview.widget.RecyclerView
    android:id="@+id/OrderHistoryRecycleView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="90dp"
    android:layout_marginStart="15dp"
    android:layout_marginEnd="15dp"
    app:layout_constraintTop_toBottomOf="@+id/actionbarOrderHistory"
    tools:layout_editor_absoluteX="0dp">

</androidx.recyclerview.widget.RecyclerView>
</androidx.constraintlayout.widget.ConstraintLayout>